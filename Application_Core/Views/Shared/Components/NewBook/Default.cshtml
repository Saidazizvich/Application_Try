<h1>Index</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Write</th>
        </tr>

    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Write</td>
            <td>
                <button class="btn btn-danger">Sil</button>
            </td>
        </tr>
    }
</table>